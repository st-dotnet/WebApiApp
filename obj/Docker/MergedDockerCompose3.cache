services:
  webapiapp:
    build:
      context: D:\Sukh Ram\WebApiApp
      dockerfile: WebApiApp\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApiApp
      target: base
    container_name: WebApiApp
    entrypoint: C:\\remote_debugger\\x64\\msvsmon.exe /noauth /anyuser /silent /nostatus
      /noclrwarn /nosecuritywarn /nofirewallwarn /nowowwarn /timeout:2147483646 /LogDebuggeeOutputToStdOut
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: c:\.nuget\fallbackpackages;c:\.nuget\fallbackpackages2
      NUGET_PACKAGES: C:\.nuget\packages
    image: webapiapp:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath c:\.nuget\packages
        --additionalProbingPath c:\.nuget\fallbackpackages --additionalProbingPath
        c:\.nuget\fallbackpackages2  "C:\app\bin\Debug\net6.0\WebApiApp.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: C:\remote_debugger\x64\utils\KillProcess.exe
        dotnet.exe
      com.microsoft.visualstudio.debuggee.program: '"C:\Program Files\dotnet\dotnet.exe"'
      com.microsoft.visualstudio.debuggee.workingdirectory: C:\app
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\ST\AppData\Roaming\ASP.NET\Https:C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https:ro
    - C:\Users\ST\AppData\Roaming\Microsoft\UserSecrets:C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets:ro
    - D:\Sukh Ram\WebApiApp\WebApiApp:C:\app:rw
    - C:\Users\ST\onecoremsvsmon\17.4.11004.1991:C:\remote_debugger:ro
    - D:\Sukh Ram\WebApiApp:C:\src:rw
    - C:\Program Files (x86)\Microsoft\Xamarin\NuGet:c:\.nuget\fallbackpackages:ro
    - C:\Program Files\DevExpress 22.1\Components\Offline Packages:c:\.nuget\fallbackpackages2:ro
    - C:\Users\ST\.nuget\packages:c:\.nuget\packages:ro
version: '3.4'